  "bugs": {
    "url": "https://github.com/dereksamuel/transformart_backend/issues"
  },
  "homepage": "https://github.com/dereksamuel/transformart_backend#readme",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "flatted": "^3.2.5",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.2.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "sequelize": "^6.16.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
